<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>cyber</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>controller.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Authentication</servlet-name>
    <servlet-class>controller.Authentication</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TypeRoomManager</servlet-name>
    <servlet-class>controller.TypeRoomManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ServiceManager</servlet-name>
    <servlet-class>controller.ServiceManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ComputerManager</servlet-name>
    <servlet-class>controller.ComputerManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TypeServiceManage</servlet-name>
    <servlet-class>controller.TypeServiceManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RoomManager</servlet-name>
    <servlet-class>controller.RoomManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CategoryManager</servlet-name>
    <servlet-class>controller.CategoryManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ProductManager</servlet-name>
    <servlet-class>controller.ProductManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BillServlet</servlet-name>
    <servlet-class>controller.BillServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BillManager</servlet-name>
    <servlet-class>controller.BillManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SearchProduct</servlet-name>
    <servlet-class>controller.SearchProduct</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Authentication</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TypeRoomManager</servlet-name>
    <url-pattern>/managerTypeRoom</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RoomManager</servlet-name>
    <url-pattern>/managerRoom</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CategoryManager</servlet-name>
    <url-pattern>/managerCategory</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ProductManager</servlet-name>
    <url-pattern>/managerProduct</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BillServlet</servlet-name>
    <url-pattern>/huy</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BillManager</servlet-name>
    <url-pattern>/managerBill</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SearchProduct</servlet-name>
    <url-pattern>/search</url-pattern>
  </servlet-mapping>
</web-app>